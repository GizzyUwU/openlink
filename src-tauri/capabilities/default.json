{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-close",
    "core:window:allow-toggle-maximize",
    "core:window:allow-minimize",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "https://*.edulinkone.com/**"
        },
        {
          "path": "blob:**"
        }
      ]
    },
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.edulinkone.com"
        },
        {
          "url": "http://localhost:*/"
        }
      ]
    },
    "keyring:default",
    "store:default",
    "fs:allow-resource-read-recursive",
    "fs:allow-exists",
    "store:allow-get",
    "store:allow-set",
    "store:allow-delete",
    "store:allow-keys",
    "store:allow-clear"
  ]
}
