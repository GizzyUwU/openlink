{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.edulinkone.com"
        },
        {
          "url": "http://localhost:*/"
        }
      ]
    },
    "keyring:default",
    "store:default",
    "fs:allow-resource-read-recursive",
    "fs:allow-exists"
  ]
}
